    <!-- HEADER -->
    <b:section class='header' id='header' maxwidgets='1' showaddelement='yes'>
      <b:widget id='Header1' locked='true' mobile='no' title='COMICORP (cabecera)' type='Header' visible='false'>
        <b:widget-settings>
          <b:widget-setting name='displayUrl'/>
